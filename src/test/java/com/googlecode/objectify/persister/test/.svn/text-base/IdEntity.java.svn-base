package com.googlecode.objectify.persister.test;

import javax.persistence.Id;

import com.googlecode.objectify.annotation.Entity;

@Entity
public abstract class IdEntity {

	@Id
	private Long id;

	public IdEntity() {
		super();
	}

	public Long getId() {
		return id;
	}

	public void setId(Long id) {
		this.id = id;
	}

}